{
  "name": "VK-Data",
  "version": "2.2.4",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "VK-Data",
    "manifest": "src/manifest.json",
    "main": "vkdata.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.16",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@skpm/extract-loader": "^2.0.2",
    "@skpm/fs": "^0.2.4",
    "mocha-js-delegate": "^0.1.1",
    "sketch-module-google-analytics": "^0.2.0",
    "skpm": "^1.1.15"
  },
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VKCOM/vkdata-sketchplugin.git"
  },
  "homepage": "https://github.com/VKCOM/vkdata-sketchplugin",
  "bugs": {
    "url": "https://github.com/VKCOM/vkdata-sketchplugin/issues"
  }
}
